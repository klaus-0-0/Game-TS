{
  "compilerOptions": {
    "rootDir": "./src",
    "outDir": "./dist",
    "module": "node16",
    "target": "ES2023",
    "types": [
      "node"
    ],
    "moduleResolution": "node16",
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": true,
    "skipLibCheck": true,
    "moduleDetection": "force",
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src"
, "src/games/17CardGame"  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}